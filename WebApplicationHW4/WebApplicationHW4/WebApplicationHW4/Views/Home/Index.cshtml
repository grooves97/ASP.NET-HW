@{
    ViewBag.Title = "Home Page";
    var news = ViewBag.News;
}

<h2>Index</h2>


<div class="row">
    @foreach (var item in news)
    {
        <div>
            <img src="@item.ImagePath"/>
            <div>
                <h2>@item.Title</h2>
                <p>@item.Text</p>
            </div>
        </div>
    }
</div>